---
import type { ILayoutProps } from "@interfaces/layout-props"
import BaseLayout from "./BaseLayout.astro"

import { ContentDiv } from "@components/content-div"

export interface Props extends ILayoutProps {}

let { title, tab } = Astro.props
---

<BaseLayout title={title} tab={tab}>
  <ContentDiv slot="main">
    <slot name="main" />
  </ContentDiv>
</BaseLayout>
